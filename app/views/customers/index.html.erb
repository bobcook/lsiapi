<p id="notice"><%= notice %></p>

<h1>Listing Customers</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Address</th>
      <th>City</th>
      <th>Region code</th>
      <th>Country code</th>
      <th>Zip</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Access</th>
      <th>Active</th>
      <th>Boolean</th>
      <th>Modified date</th>
      <th>Email template</th>
      <th>Rate</th>
      <th>Entity</th>
      <th>Name file</th>
      <th>Customer file</th>
      <th>Billing</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @customers.each do |customer| %>
      <tr>
        <td><%= customer.first_name %></td>
        <td><%= customer.last_name %></td>
        <td><%= customer.address %></td>
        <td><%= customer.city %></td>
        <td><%= customer.region_code %></td>
        <td><%= customer.country_code %></td>
        <td><%= customer.zip %></td>
        <td><%= customer.phone %></td>
        <td><%= customer.email %></td>
        <td><%= customer.access %></td>
        <td><%= customer.active %></td>
        <td><%= customer.boolean %></td>
        <td><%= customer.modified_date %></td>
        <td><%= customer.email_template %></td>
        <td><%= customer.rate %></td>
        <td><%= customer.entity %></td>
        <td><%= customer.name_file_id %></td>
        <td><%= customer.customer_file_id %></td>
        <td><%= customer.billing_id %></td>
        <td><%= link_to 'Show', customer %></td>
        <td><%= link_to 'Edit', edit_customer_path(customer) %></td>
        <td><%= link_to 'Destroy', customer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Customer', new_customer_path %>
